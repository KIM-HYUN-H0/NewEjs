              <!-- 메인 -->
<h1 class="text-center"><%= title %></h1>
<p class="text-right"><a href="/visited/<%= number %>/write" class="btn btn-success" >글쓰기</a></p>
<div class="container">
<table class="table table-hover">
    <tr>
        <td>번호</td>
        <td>작성자</td>
